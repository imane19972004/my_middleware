<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<system.serviceModel>
		<services>
			<service name="RoutingServer.RoutingService">
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8003/RoutingServer"/>
					</baseAddresses>
				</host>
				<endpoint address="" binding="basicHttpBinding" contract="RoutingServer.IRoutingService"/>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
			</service>
		</services>
	</system.serviceModel>
</configuration>